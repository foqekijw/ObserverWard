{
    "interval_seconds": 10,
    "gemini_model": "gemini-2.5-flash-lite",
    "screenshot_width": 1280,
    "screenshot_height": 720,
    "screenshot_monitor_index": 1,
    "hash_method": "phash",
    "hash_threshold": 7,
    "stable_window_seconds": 60,
    "only_on_change": true,
    "cache_ttl_seconds": 45,
    "disable_cache": true,
    "minute_limit": 8,
    "day_limit": 10000,
    "alert_threshold_percent": 80,
    "retry_max_attempts": 3,
    "retry_initial_delay": 1.0,
    "retry_backoff_factor": 2.0,
    "disable_retries": false,
    "response_format": "comment_only",
    "details_use_style": true,
    "strict_interval": true,
    "silent_mode": false,
    "temperature": 1,
    "timeout_ms": 30000,
    "max_output_tokens": 1024,
    "response_modalities": [
        "text"
    ],
    "context_history_size": 3,
    "history_path": ".ai_commentator_history.json",
    "log_path": "ai_commentator.log",
    "errors_dir": "errors",
    "log_every_n_calls": 5,
    "menu_default_interval": 10,
    "menu_default_style": "1",
    "subtitle_font_family": "Helvetica",
    "subtitle_font_size_percent": 100,
    "subtitle_color": "white",
    "subtitle_past_color": "#cccccc",
    "subtitle_bg_color": "black",
    "subtitle_bg_opacity": 0
}